@model IEnumerable<Preacepta.Modelos.AbstraccionesFrond.DocsPoderesEspecialesJudicialeDTO>

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Texto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAbogadoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Texto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAbogadoNavigation.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.Apellido1)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDoc">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDoc">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDoc">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
