@model Preacepta.Modelos.AbstraccionesFrond.GeAbogadoDTO

@{
    ViewData["Title"] = "Details";
    Layout = "_Layout";
}

<h1>Detalles </h1>

<div>
    <h4>Modulo de Abogados</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Carnet)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Carnet)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CJuridicaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CJuridicaNavigation.Direccion2)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CedulaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CedulaNavigation.Apellido1)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdTipoAbogadoNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTipoAbogadoNavigation.Nombre)
        </dd>
    </dl>
</div>
<div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model?.Cedula" class="btn btn-warning" style="width: 115px; height: 40px; text-align: center; line-height: 26px;">Editar</a> 
</div>

<div class="form-group">
    <a asp-action="Index" class="btn btn-info" style="width: 115px; height: 40px; text-align: center; line-height: 26px;">Regresar</a>
</div>
