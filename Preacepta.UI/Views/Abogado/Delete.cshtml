@model Preacepta.Modelos.AbstraccionesFrond.GeAbogadoDTO

@{
    ViewData["Title"] = "Delete";
    Layout = "_Layout";
}

<h1>Eliminar</h1>

<h3>Seguro que desea eliminar este Abogado?</h3>
<div>
    <h4>TGeAbogado</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Carnet)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Carnet)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CJuridicaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CJuridicaNavigation.Direccion2)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CedulaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CedulaNavigation.Apellido1)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdTipoAbogadoNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTipoAbogadoNavigation.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Cedula" />
        <input type="submit" value="Eliminar" class="btn btn-danger" style=" width: 115px; height: 40px;" />
    </form>

    <div class="form-group">
        <a asp-action="Index" class="btn btn-info" style="width: 115px; height: 40px; text-align: center; line-height: 26px;">Regresa</a>
    </div>

</div>
