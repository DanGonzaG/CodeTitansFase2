@model IEnumerable<Preacepta.Modelos.AbstraccionesFrond.GeRedesSocialeDTO>

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LinkRedSocila)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CedulaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LinkRedSocila)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CedulaNavigation.Cedula)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdRs">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdRs">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdRs">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
