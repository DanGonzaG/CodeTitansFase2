@model IEnumerable<Preacepta.Modelos.AbstraccionesFrond.CrDistritoDTO>

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdDistrito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreDistrito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCatonNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdDistrito)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreDistrito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCatonNavigation.NombreCanton)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDistrito">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDistrito">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDistrito">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
