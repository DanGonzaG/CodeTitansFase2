@model Preacepta.Modelos.AbstraccionesFrond.DocsPagareDTO


<head>
    <link href="~/lyso/css/Daniel.css" rel="stylesheet" />
</head>

<!-- Page Title -->
<div class="page-title-area page-title-area-two title-img-one">
    <div class="d-table">
        <div class="d-table-cell">
            <div class="page-title-text">
                <h2>Generador de Documentos Legales</h2>
                <ul>
                    <li>
                        <a asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li>
                        <i class="icofont-simple-right"></i>
                    </li>
                    <li>Generador de Pagares</li>
                </ul>
            </div>
        </div>
    </div>
</div>
<!-- End Page Title -->
<body>
<div class="contact-form contact-form-two">
    <div class="container">
        <div class="contact-wrap">
            <form id="TDocsPagaresController" class="TDocsPagaresControlleri">
                <h2>Filtrar Formulario</h2>
                <div class="row justify-content-center">
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MontoNumerico" class="control-label"></label>
                                <input asp-for="MontoNumerico" class="form-control" id="idMontoNumerico" />
                                <span asp-validation-for="MontoNumerico" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="CedulaDeudor" class="control-label"></label>
                                <select asp-for="CedulaDeudor" class="form-control" asp-items="ViewBag.CedulaDeudor" id="idCedulaDeudor"></select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="SociedadDeudor" class="control-label"></label>
                                <input asp-for="SociedadDeudor" class="form-control" id="idSociedadDeudor" />
                                <span asp-validation-for="SociedadDeudor" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="CedulaJuridicaSociedad" class="control-label"></label>
                                <input asp-for="CedulaJuridicaSociedad" class="form-control" id="idCedulaJuridicaSociedad" />
                                <span asp-validation-for="CedulaJuridicaSociedad" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="AcreedorNombre" class="control-label"></label>
                                <input asp-for="AcreedorNombre" class="form-control" id="idAcreedorNombre" />
                                <span asp-validation-for="AcreedorNombre" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="CedulaJuridicaAcreedor" class="control-label"></label>
                                <input asp-for="CedulaJuridicaAcreedor" class="form-control" id="idCedulaJuridicaAcreedor" />
                                <span asp-validation-for="CedulaJuridicaAcreedor" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="AcreedorDomicilio" class="control-label"></label>
                                <input asp-for="AcreedorDomicilio" class="form-control" id="idAcreedorDomicilio" />
                                <span asp-validation-for="AcreedorDomicilio" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="FechaVencimiento" class="control-label"></label>
                                <input asp-for="FechaVencimiento" class="form-control" id="idFechaVencimiento" />
                                <span asp-validation-for="FechaVencimiento" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="InteresFormula" class="control-label"></label>
                                <input asp-for="InteresFormula" class="form-control" id="idInteresFormula" />
                                <span asp-validation-for="InteresFormula" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="InteresTasaActual" class="control-label"></label>
                                <input asp-for="InteresTasaActual" class="form-control" id="idInteresTasaActual" />
                                <span asp-validation-for="InteresTasaActual" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="InteresBase" class="control-label"></label>
                                <input asp-for="InteresBase" class="form-control" id="idInteresBase" />
                                <span asp-validation-for="InteresBase" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="LugarPago" class="control-label"></label>
                                <select asp-for="LugarPago" class="form-control" asp-items="ViewBag.LugarPago" id="idLugarPago"></select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="CedulaFiador" class="control-label"></label>
                                <select asp-for="CedulaFiador" class="form-control" asp-items="ViewBag.CedulaFiador" id="idCedulaFiador"></select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="UbicacionFirma" class="control-label"></label>
                                <input asp-for="UbicacionFirma" class="form-control" id="idUbicacionFirma" />
                                <span asp-validation-for="UbicacionFirma" class="text-danger"></span>
                            </div>
                        </div>

                    <div class="col-md-12 col-lg-12">

                        <div class="text-center">
                            <!-- Button trigger modal -->
                            <div id="btnRevisar">
                                <button type="button" id="PrevioBtn" class="contact-btn" data-bs-toggle="modal" data-bs-target="#PrevioModal">
                                    Revisar
                                </button>
                            </div>
                            <div class="form-group" id="btnCrearCaso">
                                <input type="submit" value="Crear Caso" class="contact-btn" />
                            </div>
                            <!--<button type="submit" class="contact-btn">Submit Case</button>-->
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
</body>




