@model Preacepta.Modelos.AbstraccionesFrond.CasosEvidenciaDTO
@{
    ViewData["Title"] = "Details";
    Layout = "_Layout";
}

<h1>Detalles</h1>

<div>
    <h4>Modulo de evidencias del caso</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Titulo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Titulo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Archivo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Archivo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdCaso1)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCaso1.Descripcion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdCasoNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCasoNavigation.Descripcion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdEvidencia" class="btn btn-warning" style="width: 115px; height: 40px; text-align: center; line-height: 26px;">Editar</a>
</div>

<div class="form-group">
    <a asp-action="Index" class="btn btn-info" style="width: 115px; height: 40px; text-align: center; line-height: 26px;">Regresar</a>
</div>
