                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Combustible" class="control-label"></label>
                                <select id="idCombustible" asp-for="Combustible" class="form-control" asp-items="ViewBag.Combustible"></select>
