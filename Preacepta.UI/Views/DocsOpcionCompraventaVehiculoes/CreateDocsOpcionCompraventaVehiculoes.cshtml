                                <span asp-validation-for="PlazoOpcionAnios" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="FechaInicio" class="control-label"></label>
                                <input id="idFechaInicio" asp-for="FechaInicio" class="form-control" />
                                <span asp-validation-for="FechaInicio" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MontoSenal" class="control-label"></label>
                                <input id="idMontoSenal" asp-for="MontoSenal" class="form-control" />
                                <span asp-validation-for="MontoSenal" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MonedaSenal" class="control-label"></label>
                                <input id="idMonedaSenal" asp-for="MonedaSenal" class="form-control" />
                                <span asp-validation-for="MonedaSenal" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MontoADevolver" class="control-label"></label>
                                <input id="idMontoADevolver" asp-for="MontoADevolver" class="form-control" />
                                <span asp-validation-for="MontoADevolver" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MontoAPerder" class="control-label"></label>
                                <input id="idMontoAPerder" asp-for="MontoAPerder" class="form-control" />
                                <span asp-validation-for="MontoAPerder" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="MonedaMontoPerdido" class="control-label"></label>
                                <input id="idMonedaMontoPerdido" asp-for="MonedaMontoPerdido" class="form-control" />
                                <span asp-validation-for="MonedaMontoPerdido" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="GastosTraspasoPagadosPor" class="control-label"></label>
                                <input id="idGastosTraspasoPagadosPor" asp-for="GastosTraspasoPagadosPor" class="form-control" />
                                <span asp-validation-for="GastosTraspasoPagadosPor" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="LugarFirma" class="control-label"></label>
                                <select id="idLugarFirma" asp-for="LugarFirma" class="form-control" asp-items="ViewBag.LugarFirma"></select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="HoraFirma" class="control-label"></label>
                                <input id="idHoraFirma" asp-for="HoraFirma" class="form-control" />
                                <span asp-validation-for="HoraFirma" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="FechaFirma" class="control-label"></label>
                                <input id="idFechaFirma" asp-for="FechaFirma" class="form-control" />
                                <span asp-validation-for="FechaFirma" class="text-danger"></span>
                            </div>
                        </div>


                    <div class="col-md-12 col-lg-12">

                        <div class="text-center">
                            <!-- Button trigger modal -->
                            <div id="btnRevisar">
                                <button type="button" id="PrevioBtn" class="contact-btn" data-bs-toggle="modal" data-bs-target="#PrevioModal">
                                    Revisar
                                </button>
                            </div>
                            <div class="form-group" id="btnCrearCaso">
                                <input type="submit" value="Crear Caso" class="contact-btn" />
                            </div>
                            <!--<button type="submit" class="contact-btn">Submit Case</button>-->
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
