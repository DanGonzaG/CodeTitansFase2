                                <label asp-for="TipoVehiculo" class="control-label"></label>
                                <select id="idTipoVehiculo" asp-for="TipoVehiculo" class="form-control" asp-items="ViewBag.TipoVehiculo"></select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="ModeloVehiculo" class="control-label"></label>
                                <input id="idModeloVehiculo" asp-for="ModeloVehiculo" class="form-control" />
                                <span asp-validation-for="ModeloVehiculo" class="text-danger"></span>
                            </div>
                        </div>
