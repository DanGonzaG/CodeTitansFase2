@model Preacepta.Modelos.AbstraccionesFrond.GePersonaDTO

<head>
    <link href="~/lyso/css/Daniel.css" rel="stylesheet" />
</head>
<!-- Page Title -->
<body>
    <div class="page-title-area page-title-area-two title-img-one">
        <div class="d-table">
            <div class="d-table-cell">
                <div class="page-title-text">
                    <h2>Registrar Nuevo Cliente</h2>
                    <ul>
                        <li>
                            <a asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li>
                            <i class="icofont-simple-right"></i>
                        </li>
                        <li>Crear Cliente</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- End Page Title -->

    <br />
    <br />
    <br />


    <div class="contact-form contact-form-two contact-form-three">
        <div class="container">
            <div class="contact-wrap">
                <form asp-action="CrearPersona">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <h2>Formulario para crear cliente</h2>
                    <div class="row justify-content-center">

                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Cedula" class="control-label"></label>
                                <input asp-for="Cedula" class="form-control" />
                                <span asp-validation-for="Cedula" class="text-danger"></span>

                                @if (TempData["ErrorCedula"] != null)
                                {
                                    <span class="text-danger">@TempData["ErrorCedula"]</span>
                                }
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Nombre" class="control-label"></label>
                                <input asp-for="Nombre" class="form-control" />
                                <span asp-validation-for="Nombre" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Apellido1" class="control-label"></label>
                                <input asp-for="Apellido1" class="form-control" />
                                <span asp-validation-for="Apellido1" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Apellido2" class="control-label"></label>
                                <input asp-for="Apellido2" class="form-control" />
                                <span asp-validation-for="Apellido2" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="FechaNacimiento" class="control-label"></label>

                                <input id="InputFechaNacimiento" type="date" asp-for="FechaNacimiento" class="form-control" />
                                <span asp-validation-for="FechaNacimiento" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Edad" class="control-label"></label>

                                <input id="InputEdad" asp-for="Edad" class="form-control" />
                                <span asp-validation-for="Edad" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="EstadoCivil" class="control-label"></label>
                                <select asp-for="EstadoCivil" asp-items="ViewBag.EstadoCivil" class="form-control"></select>
                                <span asp-validation-for="EstadoCivil" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Oficio" class="control-label"></label>
                                <input asp-for="Oficio" class="form-control" />
                                <span asp-validation-for="Oficio" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">

                                <label for="provincia">Provincia:</label>
                                <select class="selectDaniel" id="provincia" name="provincia">
                                    <option value="">Seleccione un cantón</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                              
                                <label for="canton">Cantón:</label>
                                <select class="selectDaniel" id="canton" name="canton" disabled>
                                    <option value="">Seleccione un cantón</option>
                                </select>

                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label for="distrito">Distrito:</label>
                                <select class="selectDaniel" id="distrito" name="Direccion1" asp-for="Direccion1" disabled>
                                    <option value="">Seleccione un distrito</option>
                                </select>

                            </div>
                        </div>
                        <!--<div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Direccion1" class="control-label"></label>
                                <select asp-for="Direccion1" class="form-control" asp-items="ViewBag.Direccion1" style="overflow:auto"></select>
                            </div>
                        </div>-->
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Direccion2" class="control-label"></label>
                                <input asp-for="Direccion2" class="form-control" />
                                <span asp-validation-for="Direccion2" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Telefono1" class="control-label"></label>
                                <input asp-for="Telefono1" class="form-control" />
                                <span asp-validation-for="Telefono1" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label asp-for="Telefono2" class="control-label"></label>
                                <input asp-for="Telefono2" class="form-control" />
                                <span asp-validation-for="Telefono2" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <!--<div class="form-floating mb-3">-->
                            <div class="form-group">
                                <label asp-for="Email">Correro electrónico</label>
                                <input asp-for="Email" class="form-control" autocomplete="username" aria-required="true" placeholder="name@example.com" />
                                <span asp-validation-for="Email" class="text-danger"></span>
                                @if (TempData["ErrorEmail"] != null)
                                {
                                    <span class="text-danger">@TempData["ErrorEmail"]</span>
                                }
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <!--<div class="form-floating mb-3">-->
                            <div class="form-group">
                                <label asp-for="Password">Contraseña</label>

                                <input asp-for="Password" class="form-control" autocomplete="new-password" aria-required="true"/>
                                <span asp-validation-for="Password" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-6">
                            <!--<div class="form-floating mb-3">-->
                            <div class="form-group">
                                <label asp-for="ConfirmPassword">Confirmacion de contraseña</label>

                                <input asp-for="ConfirmPassword" class="form-control" autocomplete="new-password" aria-required="true"/>
                                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                            </div>
                        </div>
                        <!--<div class="col-sm-6 col-lg-6"></div>
                        <div class="col-sm-6 col-lg-6"></div>
                        <br />
                        <br />
                        <br />
                        <br />
                        <div class="col-md-12 col-lg-12">
                            <div class="form-group">
                                <textarea name="Descripcion" id="Descripcion" cols="30" rows="8" asp-for="Descripcion" placeholder="Descripción" class="form-control"></textarea>
                                <span asp-validation-for="Descripcion" class="text-danger"></span>
                            </div>
                        </div>-->

                        <div class="col-md-12 col-lg-12">

                            <div class="text-center">
                                <div class="form-group">
                                    <input type="submit" value="Create" class="contact-btn" />
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</body>


<script src="~/lyso/js/calculoedad.js"></script>
<script src="~/lyso/js/filtroprovinicacantondistrito.js"></script>








